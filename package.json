{
  "name": "remark-codesandbox-newest",
  "version": "1.0.1-beta.1",
  "description": "Create CodeSandbox directly from code blocks",
  "main": "src/browser/index.ts",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "test": "mocha",
    "test:watch": "mocha --watch --clean-cache",
    "start": "node --import tsx dist/main.js",
    "pub:beta": "npm version prerelease --preid=beta",
    "pub:patch": "npm version patch",
    "type-check": "tsc --noEmit",
    "rm": "rimraf dist && rimraf types",
    "build": "swc src -d dist --source-maps -D --strip-leading-paths",
    "build:type": "tsc --emitDeclarationOnly",
    "build:clean": "npm run rm && npm run build",
    "build:prod": "npm run type-check && npm run build:type && npm run build",
    "lint": "eslint --config .eslintrc . --cache --debug",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.{ts,js}\""
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.11",
    "@types/chai": "^4.3.14",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "chai": "^5.1.0",
    "eslint": "8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "react-markdown": "^9.0.1",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "unified": "^11.0.4",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.13.7",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "codesandbox": "^2.2.3",
    "codesandbox-import-util-types": "^2.2.3",
    "codesandbox-import-utils": "^2.2.3",
    "isomorphic-fetch": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark": "^15.0.1",
    "typescript": "^5.4.4",
    "unist-builder": "^4.0.0"
  }
}
